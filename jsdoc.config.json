{
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
      "include": ["./src"],
      "exclude": [
        "node_modules",
        "dev",
        "dist",
        "build",
        "test",
        "tests",
        "./src/test"
    ],
    "includePattern": ".js$",
    "excludePattern": "(test|mock|\\.test\\.js)$"
    },
    "plugins": [],
    "templates": {
      "cleverLinks": false,
      "monospaceLinks": false
    },
    "opts": {
      "destination": "./docs/api",
      "recurse": true,
      "verbose": true
    },
    "sourceType": "script"
}